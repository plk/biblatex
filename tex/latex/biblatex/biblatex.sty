    \blx@xml@section{0}%
  }%
  \immediate\openout\blx@auxout\jobname.bcf\relax
  \blx@auxwrite\blx@auxout{}{\blx@tempa}%
  \global\undef\blx@tempa
  \endgroup
  \AfterEndDocument{%
    \blx@auxwrite\blx@auxout{}{%
      \blx@xml@endsection
      \blx@xml@endfile}%
    \immediate\closeout\blx@auxout}}

