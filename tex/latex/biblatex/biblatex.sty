    }%
    \blx@xml@comment{BIBLATEX OPTIONS}%
    \blx@bcf@options@global
  }%
  \ifdef\blx@opts@type
    {\def\do##1{%
       \begingroup
       \blx@setoptions@type{##1}%
       \begingroup
       \let\protect\relax
       \blx@checkoptions@type
       \endgroup
       \xappto\blx@tempa{\blx@bcf@options@type{##1}}%
       \endgroup}%
